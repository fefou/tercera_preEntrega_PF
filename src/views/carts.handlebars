<h2>CARRITOS</h2>
<hr>
<div>
    {{#each carts}}   
        <p>Id: {{this._id}}</p> 
        <p>Productos:</p>
        <ul>
            {{#each this.products}}
                <li>
                    {{!-- Acceder a las propiedades del objeto product --}}
                    first_name: {{this.product.title}}<br>
                    Cantidad: {{this.quantity}}
                </li>
            {{/each}}
        </ul>
        <hr>
    {{/each}}
</div>

